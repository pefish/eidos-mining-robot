{
  "compilerOptions": {
    "allowJs": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es5",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "lib",
    "baseUrl": ".",
    "lib": ["es2015", "es2016", "es2017"],
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@pefish"
    ]
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules/**/*"
  ]
}